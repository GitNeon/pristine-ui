{
  "name": "pristine-ui",
  "version": "0.0.1",
  "description": "A Component Library for Vue 3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GitNeon/pristine-ui.git"
  },
  "keywords": [
    "pristine-ui",
    "vue3 components",
    "ui library",
    "ui framework"
  ],
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.mjs",
      "require": "./lib/index.js"
    },
    "./es": {
      "types": "./es/index.d.ts",
      "import": "./es/index.mjs"
    },
    "./lib": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js"
    }
  },
  "main": "lib/index.js",
  "module": "es/index.mjs",
  "types": "es/index.d.ts",
  "scripts": {
    "dev": "vite --config ../../vite.config.ts",
    "build": "vite build --config ../../vite.config.ts"
  },
  "peerDependencies": {
    "vue": "^3.4.21"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ]
}
