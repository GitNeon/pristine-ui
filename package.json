{
  "name": "pristine-ui",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.15.4",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm run -C packages/pristine-ui dev",
    "build": "pnpm run -C packages/pristine-ui build && node script/post-task.cjs",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "dependencies": {
    "@vue/runtime-core": "^3.4.29",
    "vue": "^3.4.21",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.1",
    "@pristine-ui/components": "workspace:*",
    "@pristine-ui/constants": "workspace:*",
    "@pristine-ui/hooks": "workspace:^",
    "@pristine-ui/utils": "workspace:^",
    "@types/node": "^20.14.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "eslint": "^8.56.0",
    "eslint-plugin-format": "^0.1.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.1",
    "vue-tsc": "^2.0.21"
  },
  "lint-staged": {
    "*.{js,ts,vue,cjs,mjs,json}": [
      "eslint --fix"
    ]
  }
}
